{
  "version": 2,
  "builds": [
    {
      "src": "dist_be/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist_be/**", "prisma/**", "node_modules/.prisma/**"],
        "maxDuration": 10
      }
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist_be/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
