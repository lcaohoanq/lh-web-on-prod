{
  "version": 2,
  "builds": [
    {
      "src": "dist_be/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist_be/**"] }
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist_be/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}